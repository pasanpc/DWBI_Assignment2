  <DataSets>
    <DataSet Name="DataSetReviews">
      <Query>
        <DataSourceName>DS_Assignment_DW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BrewType">
            <Value>=Parameters!BrewType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select db.beer_name, dbs.beer_style,
db.beer_abv,
dbr.brewery_name, dbr.brewery_type, dd.Month, dd.MonthName,
dd.Year, fr.review_overall, fr.review_appearance, fr.review_palate, fr.review_score
from FactReviews fr
inner join DimBeer db on fr.BeerKey= db.beerSK
inner join DimBeerStyle dbs
on db.beerStyleKey = dbs.StyleSK
inner join DimCustomer dc
on fr.CustomerKey = dc.CustomerSK
inner join DimBrewery dbr
on fr.BreweryKey = dbr.brewerySK
inner join DimDate dd
on fr.ReviewDateKey = dd.DateKey
where dbr.brewery_type= @BrewType</CommandText>
      </Query>
